{
  "name": "mrbabbs.com",
  "version": "1.15.0",
  "description": "Personal Website and CV",
  "main": "index.js",
  "scripts": {
    "start": "resume serve --theme jsonresume-theme-stackoverflow  -r ./resume.json",
    "export": "npm i -g ./node_modules/jsonresume-theme-stackoverflow && resume export index -f html -t stackoverflow && cp ./node_modules/jsonresume-theme-stackoverflow-FA5/public/index.html .",
    "preversion": "npm run export && git add ./index.html && git commit -m 'Add new build version of CV'",
    "validate": "resume test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrbabbs/mrbabbs.com.git"
  },
  "keywords": [
    "CV",
    "JSON",
    "resume"
  ],
  "author": "mrbabbs (Giovanni Esposito gioesps@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrbabbs/mrbabbs.com/issues"
  },
  "homepage": "https://github.com/mrbabbs/mrbabbs.com#readme",
  "devDependencies": {
    "resume-cli": "^3.0.8"
  },
  "dependencies": {
    "jsonresume-theme-stackoverflow": "^2.1.0"
  }
}
